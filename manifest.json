{
    "manifest_version": 3,
    "name": "Codeforces Random Problem Picker",
    "version": "1.0",
    "description": "Pick a random Codeforces problem based on input rating",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "permissions": [
      "activeTab",
      "storage"
    ],
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  